{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"~f/*": [
				"./src/*"
			],
			"~b/*": [
				"../backend/src/*"
			]
		},
		"typeRoots": [
			"./node_modules/@types",
			"./typings",
			"vite/client"
		]
	},
	"include": [
		"./src",
		"typings"
	]
}